{
  "name": "myfinances-api",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "arthreis"
  },
  "scripts": {
    "build": "tsc",
    "dev:server": "tsx --env-file=.env --inspect --ignore-watch node_modules -- src/shared/infra/http/server.ts",
    "prod:server": "tsx --env-file=.env.production --inspect --ignore-watch node_modules -- src/shared/infra/http/server.ts",
    "start": "tsx watch src/shared/infra/http/server.ts",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "typeorm3": "npm run typeorm migration:run -- -d src/shared/infra/typeorm/config/datasources/PostgresDatasource.ts",
    "migration:create": "tsx ./node_modules/.bin/typeorm migration:create",
    "migration:generate": "tsx ./node_modules/.bin/typeorm migration:generate -d src/shared/infra/typeorm/config/datasources/PostgresDatasource.ts",
    "migration:run": "tsx ./node_modules/.bin/typeorm migration:run -d src/shared/infra/typeorm/config/datasources/PostgresDatasource.ts",
    "migration:revert": "tsx ./node_modules/.bin/typeorm migration:revert -d src/shared/infra/typeorm/config/datasources/PostgresDatasource.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/csv-parse": "^1.2.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express-serve-static-core": "4.17.35",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^24.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.1.3"
  }
}
