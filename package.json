{
  "name": "myfinances-api",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "arthreis"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --env-file=.env.development src/shared/infra/http/server.ts",
    "start": "node dist/shared/infra/http/server.js",
    "docker:db": "docker compose -f docker-compose.dev.yml --env-file .env.development up -d",
    "docker:dev": "docker compose -f docker-compose.yml --env-file .env.development up --build",
    "typeorm": "tsx ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/config/datasources/ormconfig.ts",
    "migration:run:dev": "npm run typeorm -- --env-file=.env.development migration:run",
    "migration:create:dev": "npm run typeorm -- --env-file=.env.development migration:create",
    "migration:revert:dev": "npm run typeorm -- --env-file=.env.development migration:revert",
    "migration:generate:dev": "npm run typeorm -- --env-file=.env.development migration:generate",
    "db:reset:dev": "npm run typeorm -- schema:drop && npm run migration:run",
    "test": "jest --runInBand --env-file=.env.test",
    "dump:schema":  "docker compose -f docker-compose.yml --env-file .env.development run --rm $DB_NAME pg_dump -U $DB_USER -s $DB_NAME > db_schema.sql",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/csv-parse": "^1.2.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express-serve-static-core": "4.17.35",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^24.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.1.3"
  }
}
