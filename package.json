{
  "name": "myfinances-api",
  "version": "0.0.1",
  "main": "index.js",
  "type": "commonjs",
  "license": "MIT",
  "author": {
    "name": "arthreis"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "tsc:watch": "tsc --watch --noEmit",
    "dev": "NODE_ENV=development concurrently \"npm run tsc:watch\" \"tsx watch src/shared/infra/http/server.ts\"",
    "start": "node --env-file .env.production dist/shared/infra/http/server.js",
    "docker:db": "NODE_ENV=development docker compose -f docker-compose.dev.yml up -d",
    "docker:dev": "NODE_ENV=development docker compose -f docker-compose.yml up --build",
    "typeorm": "tsx ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/config/datasources/ormconfig.ts",
    "migration:run": "NODE_ENV=development npm run typeorm migration:run",
    "migration:create": "npm run typeorm migration:create",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate",
    "db:reset": "NODE_ENV=development npm run typeorm -- schema:drop && npm run migration:run",
    "test": "jest --runInBand --detectOpenHandles --coverage false",
    "test:coverage": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "dump:schema": "NODE_ENV=development docker compose -f docker-compose.yml run --rm db pg_dump -U postgres -h db -s $DB_NAME > db_schema.sql",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint:fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "csv-parse": "5.4.0",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "dotenv": "17.2.2",
    "express": "4.21.2",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "9.0.0",
    "multer": "2.0.2",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.16",
    "zod": "4.1.9"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.35.0",
    "@types/bcrypt": "6.0.0",
    "@types/cors": "2.8.13",
    "@types/csv-parse": "1.2.2",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.2",
    "@types/multer": "1.4.7",
    "@types/node": "24.3.1",
    "@types/supertest": "2.0.12",
    "concurrently": "9.2.1",
    "eslint": "9.35.0",
    "husky": "9.1.7",
    "jest": "29.5.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0"
  }
}
